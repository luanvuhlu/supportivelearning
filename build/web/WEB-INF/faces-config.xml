<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <action-listener>org.primefaces.application.DialogActionListener</action-listener>
        <navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
        <view-handler>org.primefaces.application.DialogViewHandler</view-handler>
    </application>
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.FileUploadRenderer</renderer-type>
            <renderer-class>com.eproject.learning.view.FileUploadController</renderer-class>
        </renderer>
    </render-kit>
<!--    <factory>
        <render-kit-factory>org.omnifaces.renderkit.Html5RenderKitFactory</render-kit-factory>
    </factory>-->
    <navigation-rule>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    
        <navigation-case>
            <from-outcome>courses</from-outcome>
            <to-view-id>/course.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>view-news</from-outcome>
            <to-view-id>/news.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>course</from-outcome>
            <to-view-id>/manager.xhtml?action=course</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>semester</from-outcome>
            <to-view-id>/manager.xhtml?action=semester</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>batch</from-outcome>
            <to-view-id>/manager.xhtml?action=batch</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>subject</from-outcome>
            <to-view-id>/manager.xhtml?action=subject</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>assignment</from-outcome>
            <to-view-id>/manager.xhtml?action=assignment</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>role</from-outcome>
            <to-view-id>/manager.xhtml?action=role</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>user</from-outcome>
            <to-view-id>/manager.xhtml?action=user</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>student</from-outcome>
            <to-view-id>/manager.xhtml?action=student</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>news</from-outcome>
            <to-view-id>/manager.xhtml?action=news</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>feedback</from-outcome>
            <to-view-id>/manager.xhtml?action=feedback</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>faq</from-outcome>
            <to-view-id>/manager.xhtml?action=faq</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>studentassignment</from-outcome>
            <to-view-id>/manager.xhtml?action=studentassignment</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mark</from-outcome>
            <to-view-id>/manager.xhtml?action=mark</to-view-id>
        </navigation-case>
        
    </navigation-rule>
</faces-config>
